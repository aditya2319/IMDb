{
	"name": "Name_Profession_Dim",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ParquetNamebasic",
						"type": "DatasetReference"
					},
					"name": "NameDim"
				},
				{
					"name": "ProfessionDim"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				},
				{
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "flatten2"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "surrogateKey2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          nconst as string,",
				"          primaryName as string,",
				"          birthYear as string,",
				"          deathYear as string,",
				"          primaryProfession as string,",
				"          knownForTitles as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> NameDim",
				"source(output(",
				"          nconst as string,",
				"          primaryName as string,",
				"          birthYear as string,",
				"          deathYear as string,",
				"          primaryProfession as string,",
				"          knownForTitles as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> ProfessionDim",
				"NameDim select(mapColumn(",
				"          nconst,",
				"          primaryName,",
				"          birthYear,",
				"          deathYear",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"ProfessionDim select(mapColumn(",
				"          nconst,",
				"          Profession = primaryProfession",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 derive(Profession = split(replace(replace(replace(Profession, \"[\", \"\"), \"]\", \"\"), \"\\\"\", \"\"), \",\")) ~> derivedColumn3",
				"derivedColumn3 foldDown(unroll(Profession),",
				"     mapColumn(",
				"          nconst,",
				"          Profession",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten2",
				"select1 keyGenerate(output(NameKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"flatten2 keyGenerate(output(ProfessionKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          NAMEKEY = NameKey,",
				"          NCONST = nconst,",
				"          PRIMARYNAME = primaryName,",
				"          BIRTHYEAR = birthYear,",
				"          DEATHYEAR = deathYear",
				"     ),",
				"     format: 'table') ~> sink1",
				"surrogateKey2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          PROFESSIONKEY = ProfessionKey,",
				"          PROFESSION = Profession,",
				"          NCONST = nconst",
				"     ),",
				"     format: 'table') ~> sink2"
			]
		}
	}
}